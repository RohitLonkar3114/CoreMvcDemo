@model IEnumerable<CoreMvcDemo.Models.City>;
@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-md-4">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>City Id</th>
                    <th>City Name</th>
                </tr>
            </thead>
            <tbody>
                    @foreach(var item in Model)
                    {
                           <tr>
                               <td>@item.CityId</td>
                               <td>@item.CityName</td>
                           </tr>        
                    }
            </tbody>
        </table>
    </div>
</div>
